{
    "builds": [{
        "src": "src/stock_tracker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9", "distDir": "static" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/stock_tracker/wsgi.py"
        }
    ]
}